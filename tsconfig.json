{
  "compilerOptions": {
    "noImplicitThis": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "ES2015",
    "jsx": "react",
    "allowJs": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
